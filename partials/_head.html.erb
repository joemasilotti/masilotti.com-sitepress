<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">

<title><%= title %></title>
<%= tag.meta name: "description", content: description %>

<%= tag "meta", name: "canonical", href: url %>
<%= tag "meta", name: "author", content: data.site.author %>

<%= tag "meta", name: "og:title", content: title %>
<%= tag "meta", name: "og:description", content: description %>
<%= tag "meta", name: "og:image", content: image %>
<%= tag "meta", name: "og:url", content: url %>
<%= tag "meta", name: "og:site_name", content: data.site.name %>
<meta property="og:locale" content="en_US">

<%= tag "meta", name: "twitter:creator", content: twitter %>
<%= tag "meta", name: "twitter:site", content: twitter %>
<%= tag "meta", name: "twitter:title", content: title %>
<%= tag "meta", name: "twitter:image", content: image %>
<%= tag "meta", name: "twitter:card", content: twitter_card %>

<%= stylesheet_link_tag "build/site" %>
<%= javascript_include_tag "application" %>
